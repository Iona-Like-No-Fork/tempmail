{
  "name": "le-risen/tempmail",
  "type": "lib",
  "version": "1.3.1",
  "license": "MIT",
  "homepage": "https://github.com/leRisen/tempmail",
  "description": "An unofficial API to interact with the service temp-mail.org",
  "keywords": [
    "php",
    "api",
    "temp-mail",
    "temp-mail.org"
  ],
  "authors": [
    {
      "name": "leRisen",
      "email": "lemmas.online@gmail.com",
      "homepage": "https://github.com/leRisen",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "leRisen\\tempmail\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "leRisen\\tempmail\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit --exclude-group FITS --bootstrap vendor/autoload.php"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
